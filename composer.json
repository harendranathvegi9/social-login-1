{
	"name": "smallhadroncollider/social-login",
	"description": "A package to make Social Login easier for API-centric PHP applications",
    "keywords": ["social", "login", "oauth", "auth", "oauth1", "oauth2", "twitter", "facebook"],
    "license": "MIT",
	"require": {
        "php": ">=5.5.0",
		"league/oauth1-client": "~1.6",
		"league/oauth2-client": "~1.0",
		"league/oauth2-facebook": "~1.0"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*"
	},
    "autoload": {
        "psr-4": {
            "SmallHadronCollider\\SocialLogin\\": "src/",
            "SmallHadronCollider\\SocialLogin\\Tests\\": "tests/"
        }
    },
	"config": {
		"preferred-install": "dist"
	}
}
